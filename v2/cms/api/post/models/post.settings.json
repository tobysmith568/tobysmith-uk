{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Body": {
      "type": "richtext",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "Excerpt": {
      "type": "text",
      "required": true
    },
    "PublishedAt": {
      "type": "date",
      "required": true
    }
  }
}
