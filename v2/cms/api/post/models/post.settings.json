{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Body": {
      "type": "richtext",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "Excerpt": {
      "type": "text",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "CoverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "RelatedPosts": {
      "type": "component",
      "repeatable": false,
      "component": "posts.related-posts"
    },
    "Links": {
      "type": "dynamiczone",
      "components": [
        "links.git-hub-repository",
        "links.website",
        "links.download",
        "links.npm",
        "links.nu-get"
      ]
    },
    "Tags": {
      "collection": "tags",
      "via": "posts",
      "dominant": true
    }
  }
}
